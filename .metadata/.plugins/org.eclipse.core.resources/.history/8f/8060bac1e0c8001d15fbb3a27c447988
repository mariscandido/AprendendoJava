import java.util.Scanner;

public class Exercicio_06 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		double a, b, c, tri, cir, tra, qua, ret;
		
		a = sc.nextDouble();
		b = sc.nextDouble();
		c = sc.nextDouble();
		
		tri = (a * c)/ 2.0;
		cir = (Math.pow(c, b));
		
		System.out.printf("Triangulo: %.3f");
		System.out.printf("Circulo: %.3f");
		System.out.printf("Trapezio: %.3f");
		System.out.printf("Quadrado: %.3f");
		System.out.printf("Retangulo: %.3f");
		
		sc.close();
	}

}
