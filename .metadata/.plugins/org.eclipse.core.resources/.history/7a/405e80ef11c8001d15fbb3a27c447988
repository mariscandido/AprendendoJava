import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);

		String x;
		int y;
		double w;
		x = sc.next();
		y = sc.nextInt();
		w = sc.nextDouble();
		System.out.println("Você digitou: " + x);
		System.out.println("Você digitou: " + y);
		System.out.println("Você digitou: " + w);
		
		sc.close();
	}

}
